<h1>Your Orders </h1>
{{# if orders }}
    <div class="row">
        <table class="table table-condensed">
            <thead>
            <tr>
                <th>Time</th>
                <th>Quantity</th>
                <th>Price</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            {{# each orders }}
                <tr>
                    <td>{{this.timeStamp}}</td>
                    <td>{{this.cart.totalQty}}</td>
                    <td>${{this.cart.totalPrice}}</td>
                    <td><a href="/user/{{this._id}}">Details</a></td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
{{/if}}